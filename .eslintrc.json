  "extends": ["airbnb", "airbnb/hooks"],//["eslint:recommended", "airbnb-base"],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
